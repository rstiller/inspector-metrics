{
  "name": "inspector-carbon",
  "description": "Graphite / carbon metric reporter for nodejs",
  "homepage": "https://rstiller.github.io/inspector-metrics/",
  "version": "2.6.0",
  "main": "./build/lib/metrics/index.js",
  "typings": "./build/lib/metrics/index.d.ts",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rstiller/inspector-metrics"
  },
  "keywords": [
    "metrics",
    "monitoring",
    "typescript",
    "graphite",
    "carbon"
  ],
  "engines": {
    "node": ">= 8",
    "npm": ">= 3"
  },
  "peerDependencies": {
    "inspector-metrics": ">=1.16.x"
  },
  "dependencies": {
    "graphite": "^0.1.4",
    "inspector-metrics": "^1.21.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@testdeck/mocha": "0.1.2",
    "@types/chai": "4.2.14",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "chai": "4.2.0",
    "eslint": "7.14.0",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "mocha": "8.2.1",
    "pkgsign": "0.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  },
  "files": [
    "build/lib/**/*"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile",
    "sign": "pkgsign sign .",
    "clean": "rimraf build",
    "compile": "tsc --incremental -p .",
    "deps": "npm-check --ignore @types/* --no-spinner --no-emoji --no-color",
    "lint": "eslint ./lib/**/*.ts ./test/**/*.ts",
    "prepublishOnly": "npm run build",
    "test": "mocha test/**/*Test.ts"
  },
  "mocha": {
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "reporter": "dot",
    "extension": "ts",
    "bail": true,
    "full-trace": true,
    "check-leaks": true,
    "retries": 1
  }
}
